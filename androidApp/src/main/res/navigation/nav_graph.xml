    <fragment
        android:id="@+id/profileFragment2"
        android:name="com.example.myapplication.android.ProfileFragment"
        android:label="ProfileFragment" />
    <fragment
        android:id="@+id/searchFragment2"
        android:name="com.example.myapplication.android.SearchFragment"
        android:label="SearchFragment" />
    <fragment
        android:id="@+id/treeFragment2"
        android:name="com.example.myapplication.android.TreeFragment"
